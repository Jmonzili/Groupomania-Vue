generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

model User {
    id       Int    @id @default(autoincrement())
    email    String @unique
    password String
}

model Post {
    id       Int    @id @default(autoincrement())
    // user 'test2@gmail.com'
    content  String
    imageUrl String
    // comments [String]
}
